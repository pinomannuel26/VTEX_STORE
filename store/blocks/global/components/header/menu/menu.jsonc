{
   "image#global__header-menu--imgLogo": {
        "title": "image global logo menu",
        "props": {
          "src": "/arquivos/ITGlogo.svg",
          "link": {
            "url": "/"
          }
        }
      },
      "search-bar#global__header-menu--search":{
        "props": {
          "displayMode": "clear-button",
          "placeholder": "Search products...",
          "blockClass":[
              "global__header-menu--search"
          ]
        }
      },
      "flex-layout.row#global__header-menu--wraperRow-items":{
        "title": "global menu items",
        "children": [
          "menu-item#global__header-main--item-trend",
          "menu-item#global__header-main--item-collection",
          "menu-item#global__header-main--item-offers",
          "menu-item#global__header-main--item-cyber",
          "menu-item#global__header-main--item-never"
        ],
        "props": {
          "blockClass": "global__header-menu--wraperRow-items",
          "colSizing": "auto",
          "preventVerticalStretch": true
        }
      },
      "menu-item#global__header-main--item-trend":{
        "title": "global item  menu trend",
        "props": {
          "id": "menu-item-trend",
          "type": "custom",
          "highlight": false,
          "blockClass":[
            "global__header-main--all-item"
          ],
          "itemProps": {
            "type": "internal",
            "href": "#",
            "noFollow": false,
            "tagTitle": "trend",
            "text": "Tendencias de ITGlobers"            
